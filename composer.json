{
    "name": "iben12/laravel-statable",
    "description": "Statable trait for Laravel Eloquent models",
    "keywords": [
        "laravel",
        "state",
        "event",
        "statemachine"
    ],
    "homepage": "http://github.com/iben12/laravel-statable",
    "license": "MIT",
    "authors": [
        {
            "name": "Bence Ivady",
            "email": "iben@iben.hu",
            "homepage": "http://github.com/iben12",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "sebdesign/laravel-state-machine": "^3.2"
    },
    "require-dev": {
        "mockery/mockery": "^1.2.3",
        "orchestra/testbench": "^5.19.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Iben\\Statable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Iben\\Statable\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Iben\\Statable\\ServiceProvider"
            ]
        }
    }
}
